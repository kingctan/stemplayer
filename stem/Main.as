package stem {	import flash.display.MovieClip;	import flash.events.*;	import flash.utils.*;		import stem.model.*;	import stem.views.*;		public class Main extends MovieClip {		// Constructor			public function Main() {						if ( stage ) init ();			else addEventListener(Event.ADDED_TO_STAGE, init);		}						private function init ( e:Event = null ):void		{			//trace ("--------------Start---------------");			// Get a varialbe for the data file from HTML, if this is not found just use  data/stem.xml"			var myDataFile:String = null;						try {				myDataFile = this.root.loaderInfo.parameters.file;			}			catch(errObject:Error) {				//trace(errObject.message);			}			//checked to see if we actuall have some data.  			if(myDataFile == null) {				myDataFile ="data/stem.xml"			}						new View(new Model(myDataFile), this.stage, this);					}	} 	} 